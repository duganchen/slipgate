{
  "name": "main",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "debug": "electron ./dist/main.js -remote-debugging-port=9222",
    "test": "jest",
    "build_schema": "typescript-json-schema ./tsconfig.json Maps -o ../reference/maps.schema"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.5.7",
    "eslint": "^7.7.0",
    "jest": "26.6.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@types/unzipper": "^0.10.3",
    "dotenv": "^8.2.0",
    "easydl": "^1.0.1",
    "electron": "^10.1.5",
    "electron-is-dev": "^1.2.0",
    "fast-xml-parser": "^3.17.4",
    "fuse.js": "^6.4.1",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.7",
    "unzipper": "^0.10.11"
  }
}
