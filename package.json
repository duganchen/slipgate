{
  "name": "slipgate",
  "version": "0.0.0",
  "description": "Frontend to browse, install and play single-player Quake 1 maps from Quaddicted",
  "main": "js/main.js",
  "scripts": {
    "build:scss": "sass scss:css",
    "build:ts": "tsc",
    "build": "npm run build:scss && npm run build:ts",
    "start": "electron js/main.js",
    "debug": "electron --inspect js/main.js",
    "debug-brk": "electron --inspect-brk js/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duganchen/slipgate.git"
  },
  "keywords": [
    "quake",
    "quaddicted"
  ],
  "author": "duganchen and t3rminus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duganchen/slipgate/issues"
  },
  "homepage": "https://github.com/duganchen/slipgate#readme",
  "devDependencies": {
    "@types/node-fetch": "^2.5.0",
    "electron": "6.0.0",
    "sass": "^1.22.9",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "fast-xml-parser": "^3.12.19",
    "node-fetch": "^2.6.0",
    "vue": "^2.6.10",
    "xml2js-es6-promise": "^1.1.1"
  }
}
